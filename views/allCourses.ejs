<html>
    <head>
        <title>
            All Courses
        </title>
        <link rel="stylesheet" href="/styles.css"/>
    </head>
    <body>
            <div class="container">
                <% courses.forEach((course)=>{ %>
                <div class="image">
                    <img src="<%= course.image %>">
                    <h3><%= course.courseName %></h3>
                    <h4 id="code"><%= course.courseCode %></h4>
                    <a href="/seeDetails">Details</a>
                </div>
                <% }) %>
    
            </div>
    </body>
</html>