<html>

<head>
    <title>
        Tutorial Page
    </title>
    <link rel="stylesheet" href="/styles.css" />
</head>

<body>
    <div class="CourseName">
        <h3>Course List</h3>
    </div>
    <% instractors.forEach((instractor)=>{ %>
        <div class="CourseTutorial">
            <h4>
                <%= instractor.CourseName %>
            </h4>
            <h4>
                <%= instractor.Name %>
            </h4>
            <h4>
                <%= instractor.Email %>
            </h4>
            <h4>
                <p>0<span><%= instractor.Number %></span></p>
            </h4>
        </div>
        <% }) %>
</body>

</html>